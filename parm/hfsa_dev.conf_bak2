[config]
RUN=hfsa
run_wave=no
run_ocean=no
ocean_model=mom6
domlat=25.0
domlon=-60.0
vitalsdomlat=250N
vitalsdomlon=600W
fakestormid=00L
fakestormname=NATL
fakestormorg=NHC
fakestorm_vitals={fakestormorg} {fakestormid} {fakestormname} {YMD} {HH}{min} {vitalsdomlat} {vitalsdomlon} -99 -99 -999 -999 -099 -9 -99 -999 -999 -999 -999 M

#use GEFS input both IC abd BC
ictype=gfsgrib2ab_0p25
bctype=gfsgrib2ab_0p25

[prelaunch]
basin_overrides=no
basin_conf=none.conf
no_basin_conf=none.conf

[analysis_merge]
fwd_wave_number=2
fwd_vmax_threshold=33

[forecast]
all_tasks=1260
atm_tasks=1200
ocn_tasks=60
wav_tasks=60
layoutx=36
layouty=30
write_groups=3
write_tasks_per_group=40

iau_regional=.true.

progsigma=.true.
iovr=5

epbl_mstar_scheme=OM4
use_lpt=1

lim_fac=3.0
rlmx=250.
elmx=250.
sfc_rlm=1
tc_pbl=0
fhswr=900.
fhlwr=900.
clam_deep=0.15

dt_inner=45

ntrack=0
